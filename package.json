{
  "name": "black-dashboard-pro-angular",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "build:dev-ssr": "npm run build:dev-client-and-server-bundles && npm run webpack:server",
    "webpack:server": "webpack --config webpack.server.config.js --progress --color",
    "serve:ssr": "node dist/server",
    "ssr": "npm run build:ssr && npm run serve:ssr",
    "ssr:dev": "npm run build:dev-ssr && npm run serve:ssr",
    "build:client-and-server-bundles": "ng build --prod && ng run black-dashboard-pro-angular:server:production --bundleDependencies none",
    "build:dev-client-and-server-bundles": "ng build --prod --configuration=dev && ng run black-dashboard-pro-angular:server:production --bundleDependencies none"
  },
